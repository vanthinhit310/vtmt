{
    "name": "platform/seo-helper",
    "description": "SEO helper package for Laravel CMS application",
    "type": "package",
    "autoload": {
        "psr-4": {
            "Platform\\SeoHelper\\": "src"
        }
    },
    "require": {
        "platform/platform": "*@dev",
        "platform/theme": "*@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Platform\\SeoHelper\\Providers\\SeoHelperServiceProvider"
            ],
            "aliases": {
                "SeoHelper": "Platform\\SeoHelper\\Facades\\SeoHelperFacade"
            }
        }
    }
}
