{
  "name": "platform/git-commit-checker",
  "description": "Check coding standard & code syntax with Git pre-commit hook.",
  "type": "package",
  "license": "MIT",
  "keywords": [
    "Laravel Framework",
    "Laravel CMS",
    "Laravel CMS Platform",
    "Git Commit Checker"
  ],
  "homepage": "https://laravel-cms.gistensal.com",
  "authors": [
    {
      "name": "Developer Team",
      "email": "developer-team@gistensal.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Platform\\GitCommitChecker\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Platform\\GitCommitChecker\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^7.1.3",
    "laravel/framework": "^5.6|^6|^7",
    "php-parallel-lint/php-parallel-lint": "^1.0",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1|^7.0|^8.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Platform\\GitCommitChecker\\Providers\\GitCommitCheckerServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable"
}
